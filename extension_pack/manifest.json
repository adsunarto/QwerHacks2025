{
    "manifest_version": 3,
    "name": "EcoTogether",
    "version": "1.0",
    "description": "Find scheduled routes by destination.",
    "permissions": [
      "storage",
      "identity",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  